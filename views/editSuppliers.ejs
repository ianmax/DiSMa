<%- include ./partials/header %>
<div class="container">
  <% if(hasOwnProperty('error')){ %>
    <h4 style="color: red">Please Input The Correct Email Format!</h4>
  <% } %>
  <form class="editSuppliers" action="/suppliers/edit/<%= dataJsonSuppliers[0].id %>" method="post">
    Input Suppliers First Name: <br>
    <input type="text" name="first_name" value="<%= dataJsonSuppliers[0].first_name %>">
    <br>
    Input Suppliers Last Name: <br>
    <input type="text" name="last_name" value="<%= dataJsonSuppliers[0].last_name %>">
    <br>
    Input Suppliers Email: <br>
    <input type="text" name="email" value="<%= dataJsonSuppliers[0].email %>">
    <br><br>
    <input type="submit" name="submit" value="Submit">
  </form>
</div>
  <%- include ./partials/footer %>
</body>
</html>
